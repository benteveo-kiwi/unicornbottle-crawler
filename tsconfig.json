{
    "compilerOptions": {
        "outDir": "out/",
        "module": "commonjs",
        "esModuleInterop": true,
        "target": "es6",
        "moduleResolution": "node",
        "sourceMap": true,

        "strict": true,
        "noImplicitReturns": true,
        "forceConsistentCasingInFileNames": true
    },
    "include": ["src/*.ts"]
}
